package main;

public enum Divisa {
PESO_MX("MXN - Peso mexicano"), 
DOLAR_US("USD - Dólar estadounidense"), 
EURO("EUR - Euro"), LIBRA_GB("GBP - Libra esterlina"), YEN("JPY - Yen Japonés"), WON("KRW - Won surcoreano");
	
	public final String label;
	
	private Divisa(String nombre) {
		this.label = nombre;
	}
}
