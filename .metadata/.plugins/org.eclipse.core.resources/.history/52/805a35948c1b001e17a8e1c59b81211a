package main;

import javax.swing.*;
import java.awt.Color;

public class ConverterToolsGUI {
	public static void main(String[] args) {
		JFrame converterTools = new JFrame("Conversion Tools - Challenge ONE");
		converterTools.setSize(800,600);
		
		JPanel panel = new JPanel();
		panel.setBackground(Color.RED);
		panel.setVisible(true);
		converterTools.setVisible(true);
	}
	
}
